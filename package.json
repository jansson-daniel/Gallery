{
  "name": "Gallery",
  "version": "0.0.1",
  "description": "A gallery",
  "main": "index.js",
  "author": "Daniel Jansson",
  "scripts": {
    "mongo:start": "mongod --config /usr/local/etc/mongod.conf --fork --logpath /dev/null",
    "mongo:stop": "mongo admin --eval 'db.shutdownServer()' > /dev/null",
    "build": "webpack --config ./webpack/config.prod.js --progress --profile --colors",
    "start": "npm run build && NODE_ENV=production node index.js",
    "build:clean": "rimraf dist/client/static",
    "dev": "npm run build:clean && NODE_ENV=development node index.js"
  },
  "dependencies": {
    "request": "*",
    "axios": "*",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "babel-register": "^6.9.0",
    "classnames": "*",
    "config": "^1.21.0",
    "css-loader": "^0.23.1",
    "url-loader": "*",
    "file-loader": "*",
    "hapi": "^13.4.1",
    "hapi-auth-cookie": "^6.1.1",
    "hapi-require-https": "^2.1.0",
    "hapi-webpack-plugin": "^1.3.0",
    "inert": "^4.0.0",
    "joi": "^9.0.1",
    "joi-browser": "^9.0.0",
    "joi-validation-strategy": "^0.3.3",
    "mongoose": "^4.5.3",
    "opentok": "^2.5.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.1",
    "redux": "^3.5.2",
    "redux-actions": "^0.10.0",
    "redux-simple-router": "^2.0.4",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.2",
    "shortid": "*",
    "simple-peer": "*",
    "socket.io": "*",
    "style-loader": "*",
    "webpack": "^1.13.1",
    "opentok-react": "*",
    "react-i18next": "*",
    "i18next": "*",
    "json-loader": "*",
    "image-webpack-loader": "*",
